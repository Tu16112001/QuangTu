#include <stdio.h>
int main(){
 	int a[5],i;
 	float b[10],h;
 	for(i=0;i<=4;i++){
 		printf("\nnhap vao so thu %d= ",i);
 		scanf("%d",&a[i]);
	 }
	for(i=0;i<5;i++){
	 	h=1.0*a[i];
	 	b[i]=h;
	}
	for(i=5;i<9;i++){
		b[i]=0;
	}
	for(i=0;i<5;i++){
		printf("\na[%d]=%d",i,a[i]);	
	}
	for(i=0;i<10;i++){
		printf("\nb[%d]=%f",i,b[i]);
	}
}