#include<stdio.h>

int Luythua(int n,int a){
    int s = 1;
    while (n--){
        s = s * a;
    }
    return s;
}
int main(){
    int n,a,s;
    printf("\nNhap vao so : ");
    scanf("%d",&n);
    printf("\nNhap vao m≈© : ");
    scanf("%d",&a);
    s = Luythua(n,a);
    printf("%d^%d=%d",n,a,s);
    return 0;
}