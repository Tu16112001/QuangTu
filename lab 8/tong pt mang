#include <stdio.h>
int main()
{
    int a[5], b[5], c[5];
    for(int i = 0; i< 5; i++) {
      scanf("%d", &a[i]);
      printf("%d ",a[i]);
    }
    
    printf("\n");
    for(int j = 0; j < 5; j++) {
      scanf("%d", &b[j]);
      printf("%d ",b[j]);
    }
    
    printf("\n");
    for(int k = 0; k < 5; k++) {
      c[k] = a[k] + b[k];
      printf("%d ", c[k]);
    }
    return 0;
}