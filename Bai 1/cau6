#include <stdio.h>
 
int main(){
  int n;
  scanf("%d",&n);
     printf("\nBANG CUU CHUONG\n");
    for(int i = n;i < 10;i == n){
        printf("\nBANG NHAN %d\n", i);
        for(int j = 0; j <= 10; j++){
            printf("\t%d x %d = %d\n", i, j, i*j);
        }
        printf("\nBam (Y) se tiep tuc, bam (N) se ket thuc");
        break;
        
    }
}
 
 #include <stdio.h>
 
int main(){
  int n, Y, N;
  scanf("%d%c%c",&n,&Y,&N);
     printf("\nBANG CUU CHUONG\n");
     if(n<10){
         for(int i = n;i < 10;i == n){
        printf("\nBANG NHAN %d\n", i);
        for(int j = 0; j <= 10; j++){
            printf("\t%d x %d = %d\n", i, j, i*j);
        }
        printf("\nBam (Y) se tiep tuc, bam (N) se ket thuc");
        
        break;
    }
  } else if ('Y'){
    for(int i = n;i < 10;i = n+1){
        printf("\nBANG NHAN %d\n", i);
        for(int j = 0; j <= 10; j++){
            printf("\t%d x %d = %d\n", i, j, i*j);
        }
        printf("\nBam (Y) se tiep tuc, bam (N) se ket thuc");
        break;
    }
  }
     
  
}