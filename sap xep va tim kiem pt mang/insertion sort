#include<stdio.h>
int main(){

   int i, j, temp, a[25];
   for(i=0;i<5;i++){
      scanf("%d",&a[i]);
      printf("\nNhap phan tu thu %d: ", i+1);
   }
   // Thực hiện thuật toán sắp insertion sort
   for(i=1;i<5;i++){
      temp=a[i];
      j=i-1;
      while((temp<a[j])&&(j>=0)){
         a[j+1]=a[j];
        j=j-1;
      }
      a[j+1]=temp;
   }
   printf("\nThứ tự của các phần tử được sắp xếp: ");
   for(i=0;i<5;i++)
      printf(" %d",a[i]);

   return 0;
}
